class Animal is
    var name : Integer(0)
    var age : Integer(0)
    
    method setAge(newAge : Integer) is
        age := newAge
    end
    
    method getAge : Integer => age
    
    method makeSound : Integer => Integer(0)
end

class Dog extends Animal is
    var breed : Integer(1)
    
    method makeSound : Integer => Integer(1)
    
    method setBreed(b : Integer) is
        breed := b
    end
end

class Cat extends Animal is
    var indoor : Boolean(true)
    
    method makeSound : Integer => Integer(2)
    
    method isIndoor : Boolean => indoor
end
