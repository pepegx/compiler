class Stack is
    var size : Integer(0)
    var capacity : Integer(10)
    
    method push(value : Integer) is
        if size.Less(capacity) then
            size := size.Plus(Integer(1))
        else
        end
    end
    
    method pop() is
        if size.Greater(Integer(0)) then
            size := size.Minus(Integer(1))
        else
        end
    end
    
    method isEmpty : Boolean is
        if size.Equal(Integer(0)) then
            return Boolean(true)
        else
            return Boolean(false)
        end
    end
    
    method getSize : Integer => size
end
