class Parent is
    var parentField : Integer(100)
    
    method parentMethod : Integer => parentField
end

class Child extends Parent is
    var childField : Integer(200)
    var unusedInChild : Integer(999)
    
    method childMethod() : Integer is
        var local : Integer(0)
        var unused : Integer(1)
        
        local := childField.Plus(Integer(50))
        return local
    end
    
    method optimizableMethod() : Integer is
        if true then
            return Integer(42)
        else
            return Integer(0)
        end
    end
end
