class TestIfTrue is
    method alwaysTrue() : Integer is
        if true then
            return Integer(10)
        else
            return Integer(20)
        end
    end
    
    method nestedTrue() : Integer is
        var x : Integer(0)
        if true then
            x := Integer(5)
            if true then
                return x
            else
                return Integer(999)
            end
        else
            x := Integer(100)
        end
        return x
    end
end
