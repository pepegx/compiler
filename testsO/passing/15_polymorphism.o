class Shape is
    var id : Integer(0)
    
    this(i: Integer) is
        id := i
    end
    
    method getArea : Integer is
        return Integer(0)
    end
    
    method getId : Integer is
        return this.id
    end
end

class Circle extends Shape is
    var radius : Integer(0)
    
    this(i: Integer, r: Integer) is
        id := i
        radius := r
    end
    
    method getArea : Integer is
        return this.radius.Mult(this.radius).Mult(Integer(3))
    end
    
    method getRadius : Integer is
        return this.radius
    end
end

class Rectangle extends Shape is
    var width : Integer(0)
    var height : Integer(0)
    
    this(i: Integer, w: Integer, h: Integer) is
        id := i
        width := w
        height := h
    end
    
    method getArea : Integer is
        return this.width.Mult(this.height)
    end
    
    method getWidth : Integer is
        return this.width
    end
    
    method getHeight : Integer is
        return this.height
    end
end

class Triangle extends Shape is
    var base : Integer(0)
    var height : Integer(0)
    
    this(i: Integer, b: Integer, h: Integer) is
        id := i
        base := b
        height := h
    end
    
    method getArea : Integer is
        return this.base.Mult(this.height).Div(Integer(2))
    end
end

class ShapeProcessor is
    method processShape(s: Shape) : Integer is
        return s.getArea
    end
    
    method getShapeId(s: Shape) : Integer is
        return s.getId
    end
end

