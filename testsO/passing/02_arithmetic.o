class Calculator is
    method add(a: Integer, b: Integer) : Integer is
        return a.Plus(b)
    end
    
    method subtract(a: Integer, b: Integer) : Integer is
        return a.Minus(b)
    end
    
    method multiply(a: Integer, b: Integer) : Integer is
        return a.Mult(b)
    end
    
    method divide(a: Integer, b: Integer) : Integer is
        return a.Div(b)
    end
    
    this() is
        var result1 : this.add(Integer(10), Integer(5))
        var result2 : this.subtract(Integer(10), Integer(3))
        var result3 : this.multiply(Integer(4), Integer(7))
        var result4 : this.divide(Integer(20), Integer(4))
    end
end

