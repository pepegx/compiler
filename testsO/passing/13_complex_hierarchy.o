class Animal is
    var name : Integer(0)
    var age : Integer(0)
    
    this(n: Integer, a: Integer) is
        name := n
        age := a
    end
    
    method getName : Integer is
        return this.name
    end
    
    method getAge : Integer is
        return this.age
    end
    
    method speak : Integer is
        return Integer(0)
    end
end

class Dog extends Animal is
    var breed : Integer(0)
    
    this(n: Integer, a: Integer, b: Integer) is
        name := n
        age := a
        breed := b
    end
    
    method speak : Integer is
        return Integer(1)
    end
    
    method getBreed : Integer is
        return this.breed
    end
end

class Cat extends Animal is
    var color : Integer(0)
    
    this(n: Integer, a: Integer, c: Integer) is
        name := n
        age := a
        color := c
    end
    
    method speak : Integer is
        return Integer(2)
    end
    
    method getColor : Integer is
        return this.color
    end
end

class PetOwner is
    var name : Integer(0)
    var pet : Animal(Integer(0), Integer(1))
    
    this(ownerName: Integer, petName: Integer, petAge: Integer) is
        name := ownerName
        pet := Animal(petName, petAge)
    end
    
    method getPetName : Integer is
        return pet.getName
    end
    
    method getPetAge : Integer is
        return pet.getAge
    end
end

class DogOwner extends PetOwner is
    var dog : Dog(Integer(0), Integer(1), Integer(10))
    
    this(ownerName: Integer, dogName: Integer, dogAge: Integer, dogBreed: Integer) is
        name := ownerName
        dog := Dog(dogName, dogAge, dogBreed)
        pet := dog
    end
    
    method getDogBreed : Integer is
        return dog.getBreed
    end
end

class PetShop is
    method getPetInfo(owner: PetOwner) : Integer is
        return owner.getPetName
    end
    
    method getDogInfo(owner: DogOwner) : Integer is
        var name : owner.getPetName
        var breed : owner.getDogBreed
        return name
    end
end

class PetApp is
    this() is
        var shop : PetShop()
        var owner1 : PetOwner(Integer(1), Integer(2), Integer(3))
        var owner2 : DogOwner(Integer(4), Integer(5), Integer(2), Integer(6))
        
        var info1 : shop.getPetInfo(owner1)
        var info2 : shop.getDogInfo(owner2)
        var dogBreed : owner2.getDogBreed
    end
end

