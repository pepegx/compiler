class IfOptimizations is
    method test1() : Integer is
        if true then
            return Integer(10)
        else
            return Integer(20)
        end
    end
    
    method test2() : Integer is
        if false then
            return Integer(10)
        else
            return Integer(30)
        end
    end
    
    method test3() : Integer is
        var x : Integer(5)
        if false then
            x := Integer(100)
        end
        return x
    end
end
