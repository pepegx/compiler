class Animal is
    var name : Integer(0)
    
    this(n: Integer) is
        name := n
    end
    
    method speak : Integer => Integer(0)
    
    method getName : Integer => name
end

class Dog extends Animal is
    var breed : Integer(1)
    
    this(n: Integer, b: Integer) is
        name := n
        breed := b
    end
    
    method speak : Integer => Integer(100)
    
    method getBreed : Integer => breed
end
