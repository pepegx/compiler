class Calculator is
    var memory : Integer(0)
    
    method add(a : Integer, b : Integer) : Integer is
        var result : Integer(0)
        result := a.Plus(b)
        return result
    end
    
    method subtract(a : Integer, b : Integer) : Integer is
        var result : Integer(0)
        result := a.Minus(b)
        return result
    end
    
    method store(value : Integer) is
        memory := value
    end
    
    method recall : Integer => memory
end
