class Matrix is
  var matrix : Array[Array[Integer]](Integer(0))

  this is
    matrix := Array[Array[Integer]](Integer(3))
    var i : Integer(Integer(0))
    while i.Less(Integer(3)) loop
      var row : Array[Integer]
      row := Array[Integer](Integer(3))
      var j : Integer(Integer(0))
      while j.Less(Integer(3)) loop
        row.set(j, i.Plus(j))
        j := j.Plus(Integer(1))
      end
      matrix.set(i, row)
      i := i.Plus(Integer(1))
    end
  end

  method GetElement(row: Integer, col: Integer) : Integer is
    var r : Array[Integer]
    r := matrix.get(row)
    return r.get(col)
  end

  method main is
    var val : Integer
    val := GetElement(Integer(1), Integer(2))
  end
end

